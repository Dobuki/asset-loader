{
  "name": "example",
  "type": "module",
  "scripts": {
    "start": "bun run build && bun run index.ts --watch",
    "build": "bun i && rm -rf dist && mkdir dist && bun run build-icon && bun run bundler/bundler.ts && parcel build index.html",
    "build-icon": "png-to-ico ../icon.png > dist/favicon.ico"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "baojs": "^0.2.1",
    "bun-lightningcss": "^0.2.0",
    "parcel": "^2.12.0",
    "png-to-ico": "^2.1.8",
    "react-dom": "^18.3.1",
    "serve-static-bun": "^0.5.3"
  },
  "peerDependencies": {
    "@dobuki/asset-loader": "link:@dobuki/asset-loader",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}
