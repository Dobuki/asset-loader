{
  "name": "example",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run build && bun run index.ts --watch",
    "build": "bun i && bun run build-icon && bun build --target=node ./src/index.tsx --outdir=dist",
    "build-icon": "png-to-ico ../icon.png > favicon.ico"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "baojs": "^0.2.1",
    "png-to-ico": "^2.1.8",
    "serve-static-bun": "^0.5.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "@dobuki/asset-loader": "link:@dobuki/asset-loader"
  }
}
