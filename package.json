{
  "name": "@dobuki/asset-loader",
  "version": "1.0.6",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "bun test",
    "build": "bun i && rm -rf *.tsbuildinfo && rm -rf dist/*.* && bun run bundler/bundler.ts && bun run test && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "auto-publish": "npm explore @dobuki/auto-publish -- bun run auto-publish \"$(pwd)\""
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@dobuki/auto-publish": "^1.0.7",
    "@types/bun": "latest",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.13.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "description": "Package for loading assets.",
  "keywords": [
    "bun",
    "bunjs",
    "typescript",
    "template",
    "npm",
    "np",
    "publish",
    "git"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:jacklehamster/asset-loader.git"
  },
  "homepage": "https://jacklehamster.github.io/asset-loader/",
  "author": {
    "name": "Jack Le Hamster",
    "email": "jacklehamster@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
